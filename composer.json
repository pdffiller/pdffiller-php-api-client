{
    "name" : "pdffiller/pdffiller-php-api-client",
    "description": "PHP client for pdffiller.com REST API",
    "require": {
        "league/oauth2-client": "^1.0",
        "league/uri": "^4.0",
        "symfony/translation": "^3.0",
        "illuminate/validation": "^5.2",

        "pdffiller/public-rest-api-validation-rules": "^1.0"
    },
    "require-dev": {
        "adammbalogh/key-value-store-file": "^0.5.3",
        "larapack/dd": "^1.0",
        "nesbot/carbon": "^1.21",
        "vlucas/phpdotenv": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "PDFfiller\\OAuth2\\Client\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Examples\\": "examples/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pdffiller/public-rest-api-validation-rules"
        }
    ]
}
